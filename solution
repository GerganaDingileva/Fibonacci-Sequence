namespace FibonacciSequence
{
    using System;
				
    public class Program
    {
        public static void Main()
        {
            decimal num1 = 0;
            decimal num2 = 1;
            
            decimal sum = 1;
            decimal count = 0;
            
            Console.WriteLine(num1);
            Console.WriteLine(num2);
            
            while (count <= 98)
            {
                sum = num1 + num2;
                num1 = num2;
                num2 = sum;
			          
                Console.WriteLine(sum);
                count++;
            }
        }
    }
}
